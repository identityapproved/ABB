VPN_SERVICE_PROVIDER=protonvpn
VPN_TYPE=wireguard
# Replace with your Proton-generated WireGuard private key
WIREGUARD_PRIVATE_KEY=replace_with_your_private_key

# Optional filters (comma separated)
SERVER_COUNTRIES=Netherlands
# SERVER_REGIONS=
# SERVER_CITIES=
# SERVER_HOSTNAMES=

# Tier toggles
FREE_ONLY=off
STREAM_ONLY=off
SECURE_CORE_ONLY=off
TOR_ONLY=off
PORT_FORWARD_ONLY=off

TZ=UTC
