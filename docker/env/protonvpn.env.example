# ProtonVPN credentials (WireGuard)
# Generate from https://account.proton.me/u/0/vpn/WireGuard
VPN_SERVICE_PROVIDER=protonvpn
VPN_TYPE=wireguard
WIREGUARD_PRIVATE_KEY=replace_with_your_private_key

# Optional server filters (comma separated)
SERVER_COUNTRIES=Netherlands
# SERVER_REGIONS=
# SERVER_CITIES=

# Proton tiers: set to on/off as needed
FREE_ONLY=off
STREAM_ONLY=off
SECURE_CORE_ONLY=off
TOR_ONLY=off
PORT_FORWARD_ONLY=off

# Timezone for container logs
TZ=UTC
